{
  "compilerOptions": {
	"target": "ESNext",
	"module": "node16",
	"moduleResolution": "node16",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"noImplicitAny": true,
	"strictNullChecks": true,
	"strictFunctionTypes": true,
	"noImplicitThis": true,
	"noImplicitOverride": true,
	"skipLibCheck": true,
	"baseUrl": ".",
	"paths": {
	  "@": [
		"./src"
	  ]
	},
	"outDir": "./dist"
  },
  "include": [
	"./src"
  ]
}
