{
  "compilerOptions": {
	"lib": [
	  "ESNext"
	],
	"target": "ESNext",
	"module": "nodenext",
	"moduleResolution": "nodenext",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"noImplicitAny": true,
	"strictNullChecks": true,
	"strictFunctionTypes": true,
	"noImplicitThis": true,
	"noImplicitOverride": true,
	"skipLibCheck": true,
	"outDir": "./dist",
	"jsx": "react"
  },
  "include": [
	"./src"
  ]
}
